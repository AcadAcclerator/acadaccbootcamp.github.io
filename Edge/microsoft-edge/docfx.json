{
    "build": {
        "content": [{
            "files": [
                "**/*.md", "**/**.yml"
            ],
            "exclude": [
                "**/obj/**",
                "microsoft-edge/**",
                "**/includes/**"
            ]
        }],
        "resource": [{
            "files": [
                "**/*.png",
                "**/*.jpg"
            ],
            "exclude": [
                "**/obj/**",
                "microsoft-edge/**",
                "**/includes/**"
            ]
        }],
        "overwrite": [],
        "externalReference": [],
        "globalMetadata": {
            "breadcrumb_path": "/microsoft-edge/breadcrumbs/toc.json",
            "titleSuffix": "Microsoft Edge Development",
            "uhfHeaderId": "MSDocsHeader-MSEdge",
            "extendBreadcrumb":true
        },
        "fileMetadata": {},
        "template": ["docs.html"],
        "dest": "microsoft-edge"
    }
}
